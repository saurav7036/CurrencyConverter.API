{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "ExchangeRate": {
    "Providers": [
      {
        "Name": "frankfurter",
        "BaseUrl": "https://api.frankfurter.app",
        "IsFloatingRate": false,
        "LatestRateTtlSeconds": 86400,
        "RetryPolicy": {
          "RetryCount": 3,
          "InitialDelaySeconds": 2
        }
      }
    ]
  }
}
